{
  "name": "@radhagsareees/analytics",
  "version": "1.0.0",
  "description": "TypeScript-safe analytics package for Radha G Sarees e-commerce platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm --package=tsup@8.5.0 --package=typescript@5.9.3 dlx tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "posthog-js": "^1.90.0"
  },
  "devDependencies": {
    "@radhagsareees/config": "workspace:*",
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "keywords": [
    "analytics",
    "tracking",
    "posthog",
    "typescript",
    "e-commerce"
  ],
  "author": "Radha G Sarees",
  "license": "MIT"
}