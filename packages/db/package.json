{
  "name": "@radhagsareees/db",
  "version": "1.0.0",
  "private": true,
  "description": "Database schema and utilities for Radha G Sarees",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "pnpm dlx prisma generate && tsc",
    "db:generate": "pnpm dlx prisma generate",
    "db:push": "pnpm dlx prisma db push",
    "db:pull": "pnpm dlx prisma db pull",
    "db:seed": "tsx src/seed.ts",
    "db:studio": "pnpm dlx prisma studio",
    "db:reset": "pnpm dlx prisma migrate reset",
    "supabase:types": "supabase gen types typescript --project-id $(echo $SUPABASE_URL | sed 's/.*\\.//g' | sed 's/\\..*//g') --schema public > src/types/supabase-generated.ts",
    "supabase:link": "supabase link --project-ref $(echo $SUPABASE_URL | sed 's/.*\\.//g' | sed 's/\\..*//g')",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@supabase/supabase-js": "^2.38.0",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@radhagsareees/config": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.8.9",
    "eslint": "^8.52.0",
    "prisma": "^5.6.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  }
}